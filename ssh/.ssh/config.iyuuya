Host *
  IdentityFile ~/.ssh/ids/%u.%h.id_rsa
  ServerAliveInterval 15
  ServerAliveCountMax 10

Host github.com bitbucket.org gitlab.com gitlab.deck.vm
  User git

Host gist.github.com
  HostName ssh.github.com
  User git
  Port 443
  IdentityFile ~/.ssh/ids/%u.github.com.id_rsa

Include conf.d/*

# vim: ft=sshconfig
